<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="rtoasaS3B.Page1">
    <ContentPage.Content>
        <StackLayout Style="{StaticResource stackLayoutStyle}">
            <Label Text="Semana 1: ESTILO 1" HorizontalTextAlignment="Center" FontSize="18"/>
            <Entry x:Name="txtDatoUno" Placeholder="Ingrese un texto"></Entry>
            <Entry x:Name="txtDatoDos" Placeholder="Ingrese un numero" Keyboard="Numeric"></Entry>
            <Button x:Name="btnAbrir" Text="ABRIR PAGINA 2" Clicked="btnAbrir_Clicked"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>